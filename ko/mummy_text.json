  "HT_Text_Mum_Eyei_UrukiteGoodbye01": {
    "message": "\uc774 \uc9c0\uc810\uc744 \ub118\uc5b4\uac08 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4. \ub108\ubb34 \uc704\ud5d8\ud569\ub2c8\ub2e4. <E>",
    "description": "SphinxText.xls, row 424"
  },
  "HT_Text_Mum_Eyei_UrukiteGoodbye02": {
    "message": "\ud589\uc6b4\uc744 \ube55\ub2c8\ub2e4. \uc800\ud76c \ubaa8\ub450\ub294 \ub2f9\uc2e0\uc758 \uc2b9\ub9ac\ub97c \uae30\ub300\ud569\ub2c8\ub2e4.<W 90>",
    "description": "SphinxText.xls, row 425"
  },
  "HT_Text_Mum_Eye_SetMummy_Encounter01": {
    "message": "\ub108! \ub124\uac00 \ucd5c\uadfc \ub098\ub97c \uc9dc\uc99d\ub098\uac8c \ud588\ub358 \ub188\uc774\uad70!<E>",
    "description": "SphinxText.xls, row 426"
  },
  "HT_Text_Mum_Eye_SetMummy_Encounter02": {
    "message": "\uad11\uc120\uc744 \uba48\ucd9c \ub300\ub2f4\ud568\uc744 \uac00\uc9c0\uace0 \uc788\ub2e4\uace0! \ub124\ub188\uc774 \uc5b4\ub5bb\uac8c \ud588\ub294\uc9c0 \uc5b4\uc774\uac00 \uc5c6\uc9c0\ub9cc, \uacb0\ub9d0\uc774 \ubed4\ud788 \ubcf4\uc774\ub294 \uad70...<E>",
    "description": "SphinxText.xls, row 427"
  },
  "HT_Text_Mum_Eye_SetMummy_Encounter03": {
    "message": "\uadf8\ub294 \uc774\uc81c \ub354 \uc774\uc0c1 \ubb38\uc81c\ub97c \uc77c\uc73c\ud0a4\uc9c0 \uc54a\uc744 \uac83\uc774\ub2e4. \uad11\uc120\uc744 \uc7ac \uac00\ub3d9 \uc2dc\ucf1c\ub77c!<E>",
    "description": "SphinxText.xls, row 428"
  },
  "HT_Text_Mum_Eye_SetMummy_Encounter04": {
    "message": "\uc8fc\uc778\ub2d8, \uad11\uc120\uc744 \ub2e4\uc2dc \uac00\ub3d9\uc2dc\ud0ac \uc218 \uc788\uc9c0\ub9cc... \uc548\ub3fc! \uc774\ub7f4 \uc218\ub294 \uc5c6\uc5b4!<E>",
    "description": "SphinxText.xls, row 429"
  },
  "HT_Text_Mum_Eye_SetMummy_Encounter05": {
    "message": "\uc655\uad00\uc774 \uc0ac\ub77c\uc84c\ub2e4! \uad11\uc120\uc774 \uc774\uc804\ucc98\ub7fc \uac15\ub825\ud558\uc9c0 \uc54a\uc544!<E>",
    "description": "SphinxText.xls, row 430"
  },
  "HT_Text_Mum_Eye_SetMummy_Encounter06": {
    "message": "\ubb38\uc81c \uc5c6\uc5b4. \ub0b4\uac00 \ud574\uacb0\ucc45\uc744 \ucc3e\uc744 \ub54c\uae4c\uc9c0 \ucda9\ubd84\ud788 \ub9c9\uc744 \uac83\uc774\ub2e4.<E>",
    "description": "SphinxText.xls, row 431"
  },
  "HT_Text_Mum_Sewr_Sarcophagus_Presentation01": {
    "message": "\ub098\ub294 \ubaa8\ub4e0 \uc11d\uad00\uc758 \uc655\uc774\ub2e4. \ub098\ub97c \ub78c\uc138\uc2a4\ub77c\uace0 \ubd80\ub974\ub3c4\ub85d \ud558\uc5ec\ub77c.<E>",
    "description": "SphinxText.xls, row 433"
  },
  "HT_Text_Mum_Sewr_Sarcophagus_Presentation02": {
    "message": "\ub0b4 \uc8fc\uc704\uc5d0 \uc788\ub294 \ubaa8\ub4e0 \ud683\ubd88\uc5d0 \ubd88\uc744 \ucf24 \uc218 \uc788\uc73c\uba74, \uace0\ub300\uc758 \uc720\ubb3c \ud558\ub098\ub97c \ub108\uc5d0\uac8c \uc8fc\ub3c4\ub85d \ud558\uaca0\ub2e4. <E>",
    "description": "SphinxText.xls, row 434"
  },
  "HT_Text_Mum_Sewr_Sarcophagus_Presentation03": {
    "message": "\ud558\uc9c0\ub9cc \uc870\uc2ec\ud574\ub77c, \ubd88\uc744 \ubd99\uc774\uae30\ub3c4 \uc26c\uc6b4 \uac83\ub9cc\ud07c \uc798 \uaebc\uc838\ubc84\ub9ac\uae30\ub3c4 \ud558\ub2c8\uae50.<E>",
    "description": "SphinxText.xls, row 435"
  },
  "HT_Text_Mum_Sewr_Sarcophagus_Complete01": {
    "message": "\ud760... \ucd08\uc2ec\uc790\uc5d0\uac8c \uc6b0\uc5f0\ud788 \ub530\ub974\ub294 \uc7ac\uc218\uc778\uac00 \ubcf4\uad70... <E>",
    "description": "SphinxText.xls, row 436"
  },
  "HT_Text_Mum_Sewr_Sarcophagus_Complete02": {
    "message": "\uc5b4\ucc0c\ub418\uc5c8\uac74, \ub0b4\uac00 \uc57d\uc18d\uc744 \ud558\uc600\uc73c\ub2c8, \uc774\uac83\uc744 \uc720\uc6a9\ud558\uac8c \uc4f0\uae38 \ubc14\ub780\ub2e4! <W 90>",
    "description": "SphinxText.xls, row 437"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro1a": {
    "message": "\uc544~~~!!!!!! \ubbff\uc744 \uc218 \uc5c6\uc5b4! \uc54c\uc9c0\ub3c4 \ubabb\ud558\ub294 \uc5b4\ub9b0 \ub140\uc11d\uc5d0\uac8c \ub2f9\ud558\ub2e4\ub2c8! \ub9c8\ubc95 \uc7a5\uce58\uac00 \uc6b0\ub9ac\ub97c \uc5b4\ub460\uc758 \uc131\uc73c\ub85c \ub3cc\ub824\ubcf4\ub0b4 \ubc84\ub838\uc5b4!<E>",
    "description": "SphinxText.xls, row 440"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro2a": {
    "message": "\ud760.<E>",
    "description": "SphinxText.xls, row 441"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro1b": {
    "message": "\uadf8\ub9ac\uace0 \uc774\uc81c \uc6b0\ub9ac\ub294 \uc800 \ub188\uc758 \uc0dd\uba85 \uc5d0\ub108\uc9c0\ub97c \uc774\uc6a9\ud574 \uadf8\uc758 \ubaa8\uc2b5\uc73c\ub85c\ub3c4 \ubcc0\ud560 \uc218\ub3c4 \uc5c6\uac8c \ub410\uc5b4\uc694. \uc774\uc81c \uc6b0\ub9ac\uc5d0\uac90 \ub354 \uc774\uc0c1 \uac00\uce58\uac00 \uc5c6\uc2b5\ub2c8\ub2e4. \uc81c \ubd80\ud558\ub4e4\uc744 \uc2dc\ucf1c \uadf8\ub97c \ub2e4\ub978 \uc2dc\uccb4\ub4e4\uacfc \ud568\uaed8 \uc369\uc5b4\ubc84\ub9ac\uac8c \uac10\uc625\uc73c\ub85c \ub358\uc838\ubc84\ub9ac\uaca0\uc2b5\ub2c8\ub2e4.<E>",
    "description": "SphinxText.xls, row 442"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro2b": {
    "message": "\uadf8\ub798, \uc548\ud0c0\uae5d\uac8c\ub3c4 \uadf8\ub85c\ubd80\ud130 <FC 0,128,0>\ub0a9\uace8\ub2e8\uc9c0\ub4e4<END FC>\uc744 \ubaa8\ub450 \uad6c\ud558\uae30 \uc804\uc5d0 \uc758\uc2dd\uc774 \uc911\ub2e8\ub418\uc5c8\ub2e4. \uc6b0\ub9ac\ub294 \uadf8\ub97c \ubcf5\uc81c\ud558\uae30 \uc704\ud574\uc11c \uadf8\uc758 \uc0dd\uba85 \uc5d0\ub108\uc9c0\ub97c \uc0ac\uc6a9\ud560 \uc218 \uc5c6\ub2e4.<E>",
    "description": "SphinxText.xls, row 443"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro1c": {
    "message": "<EFF><EPO>\uc608\uae30\uce58 \uc54a\uc558\ub358 \uc5ec\ud589\uc73c\ub85c \uc778\ud558\uc5ec \uc6cc\ud504 \ubd80\uc801\ub4e4\uc744 \ubd84\uc2e4\ud55c \uac83\uc774 \uc5fc\ub824\uac00 \ub429\ub2c8\ub2e4. \uc2ec\uac01\ud55c \ud0c0\uaca9\uc785\ub2c8\ub2e4...<P>\uc774 \ubd80\uc801\ub4e4 \uc5c6\uc774\ub294 \uc6b0\ub9ac\uc758 \ubaa9\ud45c\ub4e4\uc744 \ub2ec\uc131\ud558\uae30\uc5d0\ub294 \uc608\uc0c1\ubcf4\ub2e4 \uc624\ub798 \uac78\ub9b4 \uac81\ub2c8\ub2e4.<E>",
    "description": "SphinxText.xls, row 444"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro2c": {
    "message": "\uadf8\ub798 \uc544\ub9c8 \uc77c\uc2dc\uc801\uc778 \ud6c4\ud1f4\uc77c\uc9c0\ub3c4, \uadf8\ub7ec\ub098 \ud328\ubc30\ub77c? \ud558! \ubaa8\ub4e0 \uac8c \uacc4\ud68d\ub300\ub85c \uc9c4\ud589 \uc911\uc774\ub2e4. <E>",
    "description": "SphinxText.xls, row 445"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro1c1": {
    "message": "\uc8fc\uc778\ub2d8, \uc65c \uadf8\ub807\uac8c \ub9d0\uc500\ud558\uc2dc\ub294\uc9c0\uc694? <E>",
    "description": "SphinxText.xls, row 446"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro2c1": {
    "message": "<EFF><EPO>\uc774\uc81c \uadf8 \ud22c\ud0d5\uce74\uba58\uc774 \uc81c\uac70\ub418\uc5c8\uc73c\ub2c8, \ub098\ub294 \uc544\ud06c\ud5e8\ub098\ub374\uc758 \ubaa8\uc2b5\uc73c\ub85c \ubaa8\ub450\uc5d0\uac8c \ud22c\ud2b8\uac00 \uc0ac\uace0\ub85c \uc8fd\uc5c8\ub2e4\uace0 \uc124\uba85\ud560 \uac83\uc774\ub2e4.<P>\uadf8\ub7ec\uba74 <FC 128,64,0>\ub8e9\uc18c\ub974<END FC>\uc5d0\uc11c \uad8c\ub825\uc744 \uac00\uc9c0\uac8c \ub420 \uac83\uc774\ub2e4. \ub098\uba38\uc9c0\ub294 \uc218\uc6d4\ud560 \uac83\uc774\ub2e4. <E>",
    "description": "SphinxText.xls, row 447"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro1d": {
    "message": "\uc608! \uadf8\ub807\uc2b5\ub2c8\ub2e4! \ub204\uad6c\ub3c4 \ub2f9\uc2e0\uc774 \uadf8\uc758 \ud615\uc81c\uac00 \uc544\ub2c8\ub77c\uace0 \uac10\ud788 \ub9d0\ud560 \uc218 \uc5c6\uc744 \uac83\uc785\ub2c8\ub2e4!<E>",
    "description": "SphinxText.xls, row 448"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro2d": {
    "message": "\ub2f9\uc5f0\ud558\uc9c0! \ub098\uc758 \ubcc0\uc7a5\uc220\uc740 \uc644\ubcbd\ud558\ub2e4.<E>",
    "description": "SphinxText.xls, row 449"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro2e": {
    "message": "<EFF><EPO>\uc758\uc2dd\uc744 \ubc29\ud574\ud55c \uadf8 \uc5b4\ub9b0 \ub140\uc11d... \ub204\uad70\uc9c0 \uc54c\uace0 \uc788\ub2e4. \uba4d\uccad\ud558\uace0 \ub299\uc5b4\ube60\uc9c4 \ub9c8\ubc95\uc0ac, \uc784\ud638\ud15d\uc758 \uc81c\uc790\ub2e4.<P>\ud328\ubc30\ub85c \ubcf4\uc774\ub294 \uac83\uc774 \uadf8\uc758 \ubbf8\uc57d\ud55c \uc815\uc2e0\uc744 \ub354\uc6b1 \ud63c\ub780 \uc2dc\ud0a4\ub294\ub370 \uc720\uc6a9\ud558\uaca0\uc9c0. \uc2e4\uc81c\ub85c \ubb34\uc2a8 \uc77c\uc774 \ubc8c\uc774\uc9c0\uace0 \uc788\ub294 \uc9c0 \uc808\ub300 \ubaa8\ub97c \uac83\uc774\ub2e4. \uadf8\ub294 \ub0b4\uac00 \uac70\uae30 \uc788\uc5c8\ub2e4\ub294 \uac83\ub3c4 \ubaa8\ub974\uc9c0!<E>",
    "description": "SphinxText.xls, row 450"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro1e": {
    "message": "\uc8fc\uc778\ub2d8, \uacfc\uc5f0 \ucc9c\uc7ac\uc774\uc2ed\ub2c8\ub2e4! <E>",
    "description": "SphinxText.xls, row 451"
  },
  "HT_Text_Mum_PreChapter_Palace_Intro2f": {
    "message": "\uc6b0\ub9ac\ub294 \ub2f9\ubd84\uac04 \uc870\uc2ec\ud574\uc57c \ud55c\ub2e4, \ud558\uc9c0\ub9cc \uad11\uc120\uc758 \ud798\uc774 \ucda9\ubd84\ud788 \uac15\ud574\uc9c0\uba74 \uadf8 \ub204\uad6c\ub3c4 \uc6b0\ub9ac\ub97c \ub9c9\uc9c0 \ubabb\ud560 \uac83\uc774\uace0, \uc784\ud638\ud15d\uc744 \uaebe\ub294 \uac83\ub3c4 \ubc8c\ub808\ub97c \uc8fd\uc774\ub294 \uac83\ub9cc\ud07c\uc774\ub098 \uc26c\uc6b8 \uac83\uc774\ub2e4. <E>",
    "description": "SphinxText.xls, row 452"
  },
